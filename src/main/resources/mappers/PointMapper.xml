<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="lizun.mappers.PointMapper">
    <resultMap id="point" type="lizun.model.Point" autoMapping="true">

    </resultMap>

    <select id="getAllPoint" resultType="lizun.model.Point">
        SELECT P.ADRESS "address",P.phone "phone" FROM POINT P;
    </select>

</mapper>
