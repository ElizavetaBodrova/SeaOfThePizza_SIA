<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="lizun.mappers.RoleMapper">
    <resultMap id="role" type="lizun.model.Role" autoMapping="true">

    </resultMap>

    <select id="getRoleIdByName" resultType="lizun.model.Role">
        select * from role r where r.name=#{name}
    </select>

</mapper>
